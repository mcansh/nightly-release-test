{
  "name": "@mcansh/nightly-release-test",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run -ws dev",
    "build": "npm run -ws build",
    "changeset": "changeset",
    "changeset:release": "npm run build && changeset publish",
    "changeset:version": "changeset version",
    "comment": "cd ./scripts/release && node -r dotenv/config -r esbuild-register ./comment.ts",
    "publish": "node ./scripts/publish.mjs"
  },
  "dependencies": {
    "@changesets/changelog-github": "0.4.5",
    "@changesets/cli": "^2.24.2",
    "@tsconfig/node16": "1.0.2",
    "@types/node": "17.0.23",
    "@types/semver": "7.3.9",
    "fs-extra": "10.1.0",
    "prettier": "2.7.1",
    "semver": "7.3.7",
    "typescript": "4.6.3"
  }
}
