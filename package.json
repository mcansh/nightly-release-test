{
  "name": "@mcansh/nightly-release-test",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run -ws dev",
    "build": "npm run -ws build",
    "changeset": "changeset",
    "changeset:release": "npm run build && changeset publish",
    "changeset:version": "changeset version",
    "comment": "node -r dotenv/config -r esbuild-register scripts/comment.ts",
    "publish": "node ./scripts/publish.mjs"
  },
  "dependencies": {
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "^2.24.3",
    "@octokit/plugin-paginate-rest": "4.1.0",
    "@octokit/plugin-throttling": "4.2.0",
    "@octokit/rest": "19.0.4",
    "@tsconfig/node16": "1.0.3",
    "@types/node": "18.7.13",
    "@types/semver": "7.3.12",
    "dotenv": "16.0.1",
    "esbuild": "0.15.5",
    "esbuild-register": "3.3.3",
    "fs-extra": "10.1.0",
    "prettier": "2.7.1",
    "semver": "7.3.7",
    "typescript": "4.7.4"
  }
}
